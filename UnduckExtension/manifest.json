{
  "manifest_version": 3,
  "name": "Unduck Bangs",
  "version": "1.0",
  "description": "Fast DuckDuckGo bang redirects using unduck.link.",
  "omnibox": { "keyword": "ud" },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["tabs", "contextMenus", "storage", "history"],
  "action": {
    "default_popup": "popup.html"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  }
}